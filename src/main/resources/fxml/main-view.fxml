<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="TOP_CENTER" prefHeight="289.0" prefWidth="241.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.javafxpasswordgenerator.controllers.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <HBox prefHeight="100.0" prefWidth="200.0">
        <TextField fx:id="passwordTextField"/>
    </HBox>
    <Slider fx:id="passwordLengthSlider"/>
    <Button text="Generate Password"
            fx:id="generatePasswordButton"
            onMouseClicked="#onGeneratePasswordButtonClick"/>

    <opaqueInsets>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </opaqueInsets>
</VBox>
